apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  annotations:
    traefik.ingress.kubernetes.io/rewrite-target: /
  creationTimestamp: null
  name: frontend
spec:
  rules:
  - host: localhost
    http:
      paths:
      - backend:
          service:
            name: desperado-frontend
            port:
              number: 80
        path: /frontend
        pathType: Prefix
      - backend:
          service:
            name: desperado-backend
            port:
              number: 80
        path: /backend
        pathType: Prefix
status:
  loadBalancer: {}
